n index="22" value="55"/><column index="23" value="60"/><column index="24" value="96"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sexID" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="138"/><column index="2" value="166"/><column index="4" value="250"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sexID2" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths/><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sexyness" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="108"/><column index="2" value="52"/><column index="3" value="138"/><column index="4" value="52"/><column index="5" value="73"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sexyness2" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths/><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">DROP VIEW sexyness ;

CREATE VIEW sexyness as
SELECT 
    ID
  , Male
  , Poolnumber
  , Female
  , CASE 
		WHEN Male &gt; 0 THEN 'male'
		END as sex1
  , CASE 
		WHEN Female &gt; 0 THEN 'female'
		End as sex2
  , CASE WHEN Larva &gt; 0 THEN 'unknown'
		END as sex3
  , CASE WHEN Nymph &gt; 0 THEN 'unknown'
		End as sex4
  , CASE 
		WHEN Male &gt; 0 THEN 'adult'
		END as lifeStage1
  , CASE 
		WHEN Female &gt; 0 THEN 'adult'
		End as lifeStage2
  , CASE WHEN Larva &gt; 0 THEN 'larvae'
		END as lifestage3
  , CASE WHEN Nymph &gt; 0 THEN 'nymph'
		End as lifestage4		
 -- , coalesce (sex1,sex2,sex3, sex4) as sex
FROM UnionQuery2_2015_2016_2014</sql><sql name="SQL 2">DROP VIEW sexID ;

CREATE VIEW sexID as

SELECT
   ID
   ,coalesce (sex1, sex2, sex3, sex4) as sex  
   ,coalesce (lifeStage1, lifeStage2, lifestage3, lifestage4) as lifestage 
   ,TRIM ((ifnull (sex1, '') || ' ' || ifnull (sex2, ''))) as occurrenceRemarks
   FROM sexyness</sql><sql name="SQL 3 (1)">DROP VIEW Output_Ticks_14_15_16 ;

CREATE VIEW Output_Ticks_14_15_16 as

SELECT 

*

FROM
UnionQuery2_2015_2016_2014 UC
INNER JOIN sexID s ON UC.ID = s.ID
INNER JOIN CollectionSites CS ON UC.Sitecode = CS.Sitecode
</sql><sql name="SQL 7">DROP VIEW sexyness2 ;

CREATE VIEW sexyness2 as
SELECT 
    [ID/Extractioncode]
  
  , Poolnumber
  
  , CASE 
		WHEN sex = 'Male' THEN 'male'
		END as sex1
  , CASE 
		WHEN sex = 'Female' THEN 'female'
		End as sex2
  , CASE WHEN sex = 'Larva' THEN 'unknown'
		END as sex3
  , CASE WHEN sex = 'Nymph' THEN 'unknown'
		End as sex4
  , CASE 
		WHEN sex = 'Male' THEN 'adult'
		END as lifeStage1
  , CASE 
		WHEN sex = 'Female'  THEN 'adult'
		End as lifeStage2
  , CASE 
		WHEN sex = 'Larva'  THEN 'larvae'
		End as lifeStage3
 
  , CASE WHEN sex = 'Nymph' THEN 'nymph'
		End as lifestage4		
 -- , coalesce (sex1,sex2,sex3, sex4) as sex
FROM UnionQuery_2013</sql><sql name="SQL 8">DROP VIEW sexID2 ;

CREATE VIEW sexID2 as

SELECT
   [ID/Extractioncode]
   ,coalesce (sex1, sex2, sex3, sex4) as sex  
   ,coalesce (lifeStage1, lifeStage2, lifestage3, lifestage4) as lifestage 
   ,TRIM ((ifnull (sex1, '') || ' ' || ifnull (sex2, ''))) as occurrenceRemarks
   FROM sexyness2</sql><sql name="SQL 9">DROP VIEW Output_ticks_13 ;

CREATE VIEW Output_Ticks_13 as
SELECT 

*

FROM
UnionQuery_2013 UC
INNER JOIN sexID2 s ON UC.[ID/Extractioncode] = s.[ID/Extractioncode]
INNER JOIN CollectionSites CS ON UC.Sitecode = CS.Sitecode</sql><sql name="SQL 6">DROP VIEW Output_Ticks_13_all;

CREATE VIEW Output_Ticks_13_all as

SELECT 

	 [ID/ExtractionCode] as ID
		,Sitecode
		,Collectiondate
		,Genus
		,Species
		,NULL as Male
		,NULL as Female
		,NULL as Larva
		,NULL as Nymph
		,Comments
		,Extractiontype
		,Extractiondate
		,Extractor
		,Storageextract
		,Poolnumber
		,PCRcode
		,PCRdate
		,ExecutorPCR
		,StoragePCRproduct
		,RFLPdate
		,ExecutorRFLP
		,Theileriaequi
		,[Babesiacanis/caballi]
		,TBE
		,Borrelia
		,Commentslab
		,[ID/extractioncode] as [ID:1]
		,sex
		,lifestage
		,occurrenceRemarks
		,sitecode as [SiteCode:1]
		,SiteName
		,Sitetype
		,Address
		,Municipality
		,ZIPCode
		,Longitude
		,Latitutde
		
		

FROM Output_Ticks_13
</sql><sql name="all_queries.sql">
CREATE VIEW IntegratedOutput_Ticks_ITG as

SELECT * FROM Output_Ticks_13_all UNION ALL 
SELECT * FROM Output_Ticks_14_15_16</sql><current_tab id="7"/></tab_sql></sqlb_project>
